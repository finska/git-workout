<p id="notice"><%= notice %></p>

<h1>Gits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gits.each do |git| %>
      <tr>
        <td><%= link_to 'Show', git %></td>
        <td><%= link_to 'Edit', edit_git_path(git) %></td>
        <td><%= link_to 'Destroy', git, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Git', new_git_path %>
